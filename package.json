{
  "name": "rgb-connect-nodejs",
  "version": "1.0.23",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.17.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsup",
    "prepare": "npm run build",
    "test": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:browser": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest tests/browser.test.ts",
    "test:browser:serve": "npm run build && npx http-server -p 8000 -o tests/browser-test.html",
    "version:patch": "npm version patch && git push && git push --tags && npm publish --access public",
    "version:minor": "npm version minor && git push && git push --tags && npm publish --access public",
    "version:major": "npm version major && git push && git push --tags && npm publish --access public",
    "example": "node scripts/signPsbt.mjs",
    "initWallet": "node --experimental-wasm-modules scripts/initWallet.js",
    "demo": "node scripts/demo.mjs",
    "example:basic": "node example-basic-usage.js",
    "example:complete": "node example-flow.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@bitcoindevkit/bdk-wallet-node": "^0.2.0",
    "@bitcoindevkit/bdk-wallet-web": "^0.2.0",
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "axios": "^1.8.4",
    "bip32": "^5.0.0-rc.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.7",
    "ecpair": "^3.0.0",
    "os": "^0.1.2",
    "ripemd160": "^2.0.3"
  },
  "keywords": [
    "rgb",
    "rgb-protocol",
    "bitcoin",
    "sdk",
    "bdk",
    "wasm",
    "taproot",
    "assets",
    "blockchain"
  ],
  "license": "MIT"
}
